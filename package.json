{
  "name": "webservices-api",
  "version": "2.0.0",
  "description": "Api voor het beheren van electrische wagens en laadpalen",
  "main": "index.ts",
  "repository": "https://github.com/HOGENT-frontendweb/frontendweb-2425-renaudVermeiren.git",
  "license": "MIT",
  "private": true,
  "packageManager": "yarn@4.4.0",
  "scripts": {
    "start:dev": "tsx watch --env-file .env --inspect=0.0.0.0:9001 src/index.ts",
    "build": "tsc",
    "lint": "eslint src/*",
    "migrate": "yarn prisma migrate dev",
    "reseed": "yarn prisma db seed",
    "migrate:dev": "prisma migrate dev",
    "migrate:test": "env-cmd -f .env.test prisma migrate dev --skip-seed",
    "test": "env-cmd -f .env.test jest --runInBand",
    "test:coverage": "env-cmd -f .env.test jest --runInBand --coverage"
  },
  "dependencies": {
    "@koa/cors": "^5.0.0",
    "@koa/router": "^14.0.0",
    "@prisma/client": "^6.13.0",
    "@types/koa-router": "^7.4.8",
    "@types/yamljs": "^0.2.34",
    "argon2": "^0.44.0",
    "config": "^4.1.0",
    "helmet": "^8.1.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "koa": "^3.0.1",
    "koa-bodyparser": "^4.4.1",
    "koa-helmet": "^8.0.1",
    "koa2-swagger-ui": "^5.11.0",
    "prisma": "^6.13.0",
    "swagger-jsdoc": "^6.2.8",
    "winston": "^3.17.0",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@faker-js/faker": "^9.3.0",
    "@stylistic/eslint-plugin": "^5.2.3",
    "@types/config": "^3.3.5",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9",
    "@types/koa": "^3.0.0",
    "@types/koa-bodyparser": "^4.3.12",
    "@types/koa__cors": "^5.0.0",
    "@types/koa__router": "^12.0.4",
    "@types/node": "^24.2.1",
    "@types/supertest": "^6.0.2",
    "@types/swagger-jsdoc": "^6",
    "env-cmd": "^10.1.0",
    "eslint": "^9.33.0",
    "eslint-plugin-jest": "^28.9.0",
    "jest": "^29.7.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.3",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.0"
  },
  "prisma": {
    "schema": "src/data/schema.prisma",
    "seed": "tsx ./src/data/seed.ts"
  },
   "engines": {
    "node": "22.18.0",
    "yarn": "4.4.0"
  }
}
